#include <cmath>
#include <iostream>
#define distance(x1,y1,x2,y2) sqrt(((x2)-(x1))*((x2)-(x1)) + ((y2)-(y1))*((y2)-(y1)))

using namespace std;

int main(){
int x1,y1,x2,y2,x3,y3;
float dist;
cin>>x1>>y1>>x2>>y2>>x3>>y3;
distance(x1,y1,x2,y2)>distance(x1,y1,x3,y3)?cout<<endl<<x1<<","<<y1<<endl:cout<<endl<<x2<<","<<y2<<endl;
return 0;
}